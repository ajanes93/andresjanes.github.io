{
    "name": "vue3-ts",
    "version": "0.5.1",
    "scripts": {
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "check:deps": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
        "release": "node ./release.js",
        "prepare": "husky install && cp -a .husky/. .git/hooks/",
        "build": "vue-tsc --noEmit && vite build",
        "dev": "vite",
        "preview": "vite preview",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
        "format": "prettier . --ignore-path .gitignore --write",
        "test": "vitest run --reporter=junit --outputFile=junit.xml --coverage",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@mdi/js": "7.0.96",
        "date-fns": "^2.28.0",
        "vue": "^3.2.25",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.1",
        "@testing-library/vue": "^6.4.2",
        "@types/node": "^18.7.6",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "@vitejs/plugin-vue": "3.0.3",
        "@vitest/coverage-c8": "^0.22.0",
        "autoprefixer": "^10.4.2",
        "c8": "^7.11.0",
        "chalk": "^4.1.0",
        "conventional-changelog-cli": "^2.2.2",
        "enquirer": "^2.3.6",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-vue": "9.3.0",
        "execa": "^4.0.2",
        "happy-dom": "6.0.4",
        "husky": "8.0.1",
        "lint-staged": "13.0.3",
        "npm-check": "^6.0.1",
        "postcss": "^8.4.6",
        "prettier": "2.7.1",
        "semver": "^7.3.7",
        "tailwindcss": "^3.0.19",
        "typescript": "^4.4.4",
        "vite": "3.0.8",
        "vitest": "0.22.0",
        "vue-tsc": "0.40.1"
    },
    "lint-staged": {
        "*": "prettier . --ignore-path .gitignore --ignore-unknown --write",
        "src/**/*.{js,ts,vue}": "eslint --ignore-path .gitignore --fix src/"
    }
}
